{
  "expo": {
    "name": "GreenVue",
    "slug": "greentradeeu",
    "version": "1.0.0",
    "newArchEnabled": true,
    "web": {
      "favicon": "./assets/icons/playstore.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-light.jpg",
          "resizeMode": "cover",
          "backgroundColor": "#0f172b",
          "imageWidth": 200,
          "dark": {
            "image": "./assets/splash-dark.jpg",
            "resizeMode": "cover",
            "backgroundColor": "#10b880",
            "imageWidth": 200
          }
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icons/playstore.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.greenvue.android",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "monochromeImage": "./assets/adaptive-icon.png",
        "backgroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172b"
      }
    },
    "extra": {
      "eas": {
        "projectId": "e664551f-f4e4-4b18-8fa0-5f1d2db21e9e"
      }
    },
    "owner": "monkypng"
  }
}
